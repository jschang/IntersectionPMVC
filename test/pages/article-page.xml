<portlet-page 
		title="JonSchang.com" 
		base-page-uri="portlet-page://base-layout.xml" 
		xmlns="urn:Model_NodeUnmarshaller_PortletPage">
		
	<row override-id="body-content">
		<column>
			<row>
				<cell id="article-graphic" width="3" portlet-uri="portlet://image.xml"/>
				<cell id="article-description" width="9" portlet-uri="portlet://article.xml"/>
			</row>
			<row>
				<cell id="article-fulltext" width="12" portlet-uri="portlet://article.xml"/>
			</row>
		</column>
		<column> 
			<row><cell id="callout01" width="4" portlet-uri="portlet://callout.xml"/></row>
			<row><cell id="callout02" width="4" portlet-uri="portlet://callout.xml"/></row>
			<row><cell id="callout03" width="4" portlet-uri="portlet://callout.xml"/></row>
		</column>
	</row>
</portlet-page>